{
    "presets": ["es2015", "react", "stage-0"],
    "env": {
        "development": {
            "plugins": [
                "react-hot-loader/babel"
            ]
        }
    },
    "plugins": [
        "transform-decorators-legacy",
        ["import", { "libraryName": "antd", "style": true }],
        ["transform-runtime", {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
        }]
    ]
}
